- if(image.pictures.size > 0)
  - if(@str == nil)
    - @pref = "#{params[:type]}-#{@tree}-1"
  - else
    - @pref = @str
  %tr{ :id => "#{@pref}_pict"}
    %td.listItem{:class => "l#{image.level + 1}", :colspan => '5'}
      - image.pictures.each do |picture|
        .listImageBox
          -#= link_to image_tag(picture.data.url(:small), :id => "picture_#{picture.id}"), ''
          %div{ :id => "picture_#{picture.id}", :class => 'listImageItemBox' }= image_tag(picture.data.url(:small))
          %br
          .cropLine
            = picture.name
          -#= number_to_human_size(picture.data_file_size)
          -#,
          -#= picture.data_content_type
          -#%br
          -#= link_to 'editovat', '#', {:id => 'editPict', :class => "#{picture.id}"}
          = link_to 'editovat', '#', :id => "edit-pictures-#{picture.id}-#{params[:type]}", :onclick => 'editAttachment( $(this)); return false'
          |
          -#= link_to 'smazat', object_url(picture), :confirm => 'Jste si jistÃ­?', :method => :delete
          = link_to 'smazat', '#', :onclick => "deleteAssets($(this),'#{picture.id}', 'pictures', 'Picture' ); return false"    
      .clearer &nbsp;
